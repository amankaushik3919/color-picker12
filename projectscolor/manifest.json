{
    "name":"Color picker",
    "description": "A color picker extension",
    "version": "1.0",
    "manifest_version":3,
    "background":{
        "service_worker":"background.js"
    },
    "permissions":[
        "storage",
        "activeTab",
        "scripting"
    ],
    "options_page":"options.html",
    "action":{
        "default_popup":"popup.html"
    },
    "icons":{
        "16":"/picker/picker_16.png",
        "32":"/picker/picker_32.png",
        "64":"/picker/picker_48.png"
    }
}